@{
    ViewData["Title"] = "Delete Report";
}
@model Crash


<link href="~/assets/css/style.css" rel="stylesheet" />

<h3>Are you sure you want to yeet this record?</h3>
<br />

<form method="post" asp-action="DeleteCrash">
    <div class="d-flex justify-content-center">
        <button type="submit" class="btn btn-danger">Delete</button>
        <div class="col-1">     </div>
        <a asp-controller="Admin" asp-action="Crashes" class="btn btn-primary">Cancel</a>
    </div>
    <input type="hidden" asp-for="crash_id" />

</form>
